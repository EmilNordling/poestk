{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "launch",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:5001",
      "webRoot": "${workspaceRoot}/client/src",
      "breakOnLoad": true,
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "webpack:///*": "${webRoot}/../*",
        "webpack:///./*": "${webRoot}/../*",
        "webpack:///client/src/*": "${webRoot}/../src/*",
        "webpack:///./~/*": "${webRoot}/../node_modules/*"
      }
    }
  ]
}
